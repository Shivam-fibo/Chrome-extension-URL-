{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "permissions": [
      "activeTab" ,
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
  
    "action": {
      "default_popup": "popup.html"
    }
  }
  